<div class = "container">
  <div class = "row">
      <table class = "table">
        <thead>
          <tr>
            <th></th>
            <th>Name</th>
            <th>Email</th>
          </tr>
        </thead>
        <tbody>
          <% @users.each do |user| %>
          <tr>
            <td><%=image_tag user.get_image, size:"50x50" %></td>
            <td><%= user.name %></td>
            <td><%= user.email %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    <%= link_to "ログアウト", destroy_admin_session_path, class:"btn btn-md btn-danger", method: :delete %>
  </div>
</div>
